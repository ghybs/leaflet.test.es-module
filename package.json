{
  "name": "leaflet.test.es-module",
  "version": "0.0.1",
  "scripts": {
    "build": "node ./build/build.js",
    "build-r": "npm run build -- rollup",
    "build-w": "npm run build -- webpack",
    "build:plugin": "npm run build:plugin-specific && npm run build:plugin-all && npm run build:plugin-all-with-export",
    "build:plugin-specific": "npm run build-r -- testPlugin/module-specific",
    "build:plugin-all": "npm run build-r -- testPlugin/module-all",
    "build:plugin-all-with-export": "npm run build-r -- testPlugin/module-all-with-export",
    "build:browserify": "npm run build:browserify-all && npm run build:browserify-all-plugin-with-export",
    "build:browserify-all": "browserify testApp/browserify/testApp.all.js -o testApp/dist/testApp.browserify.all.js",
    "build:browserify-all-plugin-with-export": "browserify testApp/browserify/testApp.all-plugin-with-export.js -o testApp/dist/testApp.browserify.all-plugin-with-export.js",
    "build:rollup": "npm run build:rollup-all && npm run build:rollup-specific && npm run build:rollup-specific-plugin-all",
    "build:rollup-all": "npm run build-r -- testApp/rollup/all",
    "build:rollup-specific": "npm run build-r -- testApp/rollup/specific",
    "build:rollup-specific-plugin-all": "npm run build-r -- testApp/rollup/specific-plugin-all",
    "build:webpack": "npm run build:webpack-all && npm run build:webpack-specific && npm run build:webpack-specific-plugin-all",
    "build:webpack-all": "npm run build-w -- testApp/rollup/all",
    "build:webpack-specific": "npm run build-w -- testApp/rollup/specific",
    "build:webpack-specific-plugin-all": "npm run build-w -- testApp/rollup/specific-plugin-all"
  },
  "dependencies": {
    "leaflet": "git+https://github.com/ghybs/Leaflet.git#4bc81a62ef69360a0c64873d816121d46d055f51"
  },
  "devDependencies": {
    "browserify": "^15.2.0",
    "rollup": "^0.54.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.3.7",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0"
  }
}
